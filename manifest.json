{
	"name": "WR-Helper",
	"description": "records most commonly searched words on WordReference",
	"version": "1.0",
	"permissions": [
	"history"
	],
	"browser_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}